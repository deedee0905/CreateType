<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.amita.createType.ex.commision.dao.CommisionDAO">
	<insert id="insertCommisionPost" parameterType="map">
			INSERT INTO
				`commisionPost`
			(
				`userId`
				,`channelId`
				,`title`
				,`content`
				,`category`
				,`minimumPrice`
				,`maximumPrice`
				,`deadline`
				,`question`
				,`thumbnail`
				,`createdAt`
				,`updatedAt`
			)
			VALUES
			(
				#{userId}
				,#{channelId}
				,#{title}
				,#{content}
				,#{category}
				,#{minimumPrice}
				,#{maximumPrice}
				,#{deadline}
				,#{question}
				,#{thumbnail}
				,now()
				,now()
			)
			
			
	
	</insert>
	

</mapper>
